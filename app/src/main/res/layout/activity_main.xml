<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:background="@android:color/darker_gray">

    <TextView android:text="Math Calc" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/poprzednie"
        android:textColor="@android:color/black" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Large Text"
        android:id="@+id/dzialanie"
        android:layout_below="@+id/poprzednie"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/black" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/button2"
        android:width="50dp"
        android:height="50dp"
        android:layout_marginTop="56dp"
        android:layout_below="@+id/dzialanie"
        android:layout_toRightOf="@+id/button1"
        android:layout_toEndOf="@+id/button1"
        android:textColor="@android:color/holo_green_dark"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/button3"
        android:textColor="@android:color/holo_green_dark"
        android:layout_alignTop="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:layout_toEndOf="@+id/button2"
        android:layout_above="@+id/button6"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/button1"
        android:layout_alignBottom="@+id/button2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:width="50dp"
        android:height="50dp"
        android:textColor="@android:color/holo_green_dark"
        android:nestedScrollingEnabled="false"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:id="@+id/button4"
        android:width="50dp"
        android:height="50dp"
        android:layout_below="@+id/button1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/holo_green_dark"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/button5"
        android:layout_below="@+id/button2"
        android:layout_toRightOf="@+id/button4"
        android:layout_toEndOf="@+id/button4"
        android:width="50dp"
        android:height="50dp"
        android:textColor="@android:color/holo_green_dark"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/button6"
        android:layout_below="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:layout_toEndOf="@+id/button2"
        android:width="50dp"
        android:height="50dp"
        android:textColor="@android:color/holo_green_dark"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/button7"
        android:layout_below="@+id/button4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:width="50dp"
        android:height="50dp"
        android:textColor="@android:color/holo_green_dark"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/button8"
        android:layout_below="@+id/button5"
        android:layout_toRightOf="@+id/button4"
        android:layout_toEndOf="@+id/button4"
        android:width="50dp"
        android:height="50dp"
        android:textColor="@android:color/holo_green_dark"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/button9"
        android:layout_alignBottom="@+id/button8"
        android:layout_alignLeft="@+id/button6"
        android:layout_alignStart="@+id/button6"
        android:width="50dp"
        android:height="50dp"
        android:textColor="@android:color/holo_green_dark"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/button0"
        android:width="50dp"
        android:height="50dp"
        android:textColor="@android:color/holo_green_dark"
        android:layout_below="@+id/button7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="buttonNumeric" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+"
        android:id="@+id/dodaj"
        android:layout_alignTop="@+id/button3"
        android:layout_toRightOf="@+id/button3"
        android:layout_toEndOf="@+id/button3"
        android:width="50dp"
        android:height="50dp"
        android:onClick="Click"
        android:textColor="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/odejmij"
        android:layout_below="@+id/button3"
        android:layout_toRightOf="@+id/button3"
        android:layout_toEndOf="@+id/button3"
        android:width="50dp"
        android:height="50dp"
        android:onClick="Click"
        android:textColor="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*"
        android:id="@+id/pomnoz"
        android:layout_alignTop="@+id/button9"
        android:layout_toRightOf="@+id/button9"
        android:layout_toEndOf="@+id/button9"
        android:width="50dp"
        android:height="50dp"
        android:onClick="Click"
        android:textColor="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:id="@+id/podziel"
        android:layout_alignBottom="@+id/button0"
        android:layout_alignLeft="@+id/button9"
        android:layout_alignStart="@+id/button9"
        android:width="50dp"
        android:height="50dp"
        android:onClick="Click"
        android:textColor="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="."
        android:id="@+id/przecinek"
        android:layout_alignTop="@+id/button0"
        android:layout_toRightOf="@+id/button0"
        android:layout_toEndOf="@+id/button0"
        android:width="50dp"
        android:height="50dp"
        android:onClick="buttonNumeric"
        android:textColor="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="="
        android:id="@+id/rownasie"
        android:layout_alignBottom="@+id/podziel"
        android:layout_toRightOf="@+id/button9"
        android:layout_toEndOf="@+id/button9"
        android:width="50dp"
        android:height="50dp"
        android:onClick="getnumbertwo"
        android:textColor="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Czyść"
        android:id="@+id/czysc"
        android:onClick="Czyszczenie"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/holo_green_dark" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sqrt"
        android:id="@+id/sqrt"
        android:textColor="@android:color/holo_green_light"
        android:layout_below="@+id/button0"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="Click" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sin"
        android:id="@+id/sin"
        android:textColor="@android:color/holo_green_light"
        android:layout_alignTop="@+id/sqrt"
        android:layout_toRightOf="@+id/sqrt"
        android:layout_toEndOf="@+id/sqrt"
        android:onClick="Click" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cos"
        android:id="@+id/cos"
        android:layout_alignTop="@+id/sin"
        android:layout_toRightOf="@+id/sin"
        android:layout_toEndOf="@+id/sin"
        android:textColor="@android:color/holo_green_light"
        android:onClick="Click" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tan"
        android:id="@+id/tan"
        android:layout_alignTop="@+id/cos"
        android:layout_toRightOf="@+id/cos"
        android:layout_toEndOf="@+id/cos"
        android:textColor="@android:color/holo_green_light"
        android:onClick="Click" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ctan"
        android:id="@+id/ctan"
        android:layout_below="@+id/sqrt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/holo_green_light"
        android:onClick="Click" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="pi"
        android:id="@+id/pi"
        android:layout_alignTop="@+id/ctan"
        android:layout_toRightOf="@+id/ctan"
        android:layout_toEndOf="@+id/ctan"
        android:textColor="@android:color/holo_green_light"
        android:onClick="Click" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ln"
        android:id="@+id/lognat"
        android:layout_alignTop="@+id/pi"
        android:layout_toRightOf="@+id/pi"
        android:layout_toEndOf="@+id/pi"
        android:textColor="@android:color/holo_green_light" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:id="@+id/procent"
        android:layout_alignTop="@+id/lognat"
        android:layout_toRightOf="@+id/lognat"
        android:layout_toEndOf="@+id/lognat"
        android:textColor="@android:color/holo_green_light" />

</RelativeLayout>